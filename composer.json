{
    "name": "gustamms/prometheus-laravel",
    "description": "Lib for prometheus connector",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Gustamms\\PrometheusLaravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Gustavo Mendes",
            "email": "gustamms@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "promphp/prometheus_client_php": "^2.7.1",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/routing": "^6.0 || ^7.0 || ^8.0 || ^9.0"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform-check": false
    },
    "prefer-stable": false,
    "version": "v1.5"
}
