    "type": "project",
    "autoload": {
        "psr-4": {
            "Gustamms\\PrometheusLaravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Gustavo Mendes",
            "email": "gustamms@hotmail.com"
        },
        {
            "name": "Diego Ananias",
